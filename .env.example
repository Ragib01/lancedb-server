# LanceDB Server Configuration
LANCEDB_PORT=9000
LANCEDB_HOST=0.0.0.0
LANCEDB_DATA_DIR=/data
LANCEDB_LOG_LEVEL=INFO
LANCEDB_MAX_CONNECTIONS=100
LANCEDB_AUTH_ENABLED=true
LANCEDB_JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
LANCEDB_CORS_ORIGINS=*

# Database Configuration
POSTGRES_PORT=9432
POSTGRES_DB=lancedb
POSTGRES_USER=lancedb
POSTGRES_PASSWORD=change-this-password-in-production

# Redis Configuration
REDIS_PORT=9379
REDIS_PASSWORD=change-this-redis-password-in-production

# Nginx Configuration (Production)
NGINX_HTTP_PORT=90
NGINX_HTTPS_PORT=9443

# Monitoring Configuration (optional)
PROMETHEUS_PORT=9091
GRAFANA_PORT=9301
GRAFANA_PASSWORD=admin

# Production Settings (uncomment for production)
# LANCEDB_CORS_ORIGINS=https://yourdomain.com,https://app.yourdomain.com
# LANCEDB_LOG_LEVEL=WARNING 